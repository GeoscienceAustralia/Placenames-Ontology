      prof:hasArtifact <http://linked.data.gov.au/def/placenames.ttl> ;
      prof:hasRole <http://www.w3.org/ns/dx/prof/role/schema> ;
    ] ;
  prof:isProfileOf <http://www.opengis.net/doc/IS/geosparql/1.0> ;
